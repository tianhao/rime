patch:
  #  "style/candidate_format": "\u2005%c\u2005%@\u2005"  #1/6 em 空格 => U+2005
  #  主题尝试：
  #  MyStyleMi: 一般
  #  MyStyleMiDark: 不错
  #  apathy: 一般
  #  aqua: 一般
  #  azure: 不错
  #  clean_white: 一般
  #  dark_temple: 一般
  #  dust: 不行
  #  google: 不错
  #  ink: 不错
  #  lost_temple: 不错
  #  luna: 还行
  #  mojave_dark: 一般，橫向
  #  native: 不错
  #  psionics: 不错
  #  purity_of_essence: 不错
  #  purity_of_form: 不错
  #  solarized_dark: 一般
  #  solarized_light: 不错
  #  solarized_rock: 一般
  #  starcraft: 不错
  "style/color_scheme_dark": purity_of_form
  "style/color_scheme": purity_of_form
  "us_keyboard_layout": true
  "style/display_tray_icon": true
  "style/font_face": "JetBrains Mono Medium"
  "style/comment_font_point": 18               # 注释字体大小
  "style/font_point": 20                       # 全局字体大小
  "style/label_font_point": 20                 # 候选词数字大小
  "style/horizontal": false       # 丛向展示
  "style/spacing": 2              # 候选词行距 横向
  "style/inline_preedit": true    # 内嵌预编辑，#单行显示，false双行显示
  "style/layout/border_width": 6           # 窗口边界高度
  "style/layout/candidate_spacing": 6
  "style/layout/hilite_padding": 7         # 高亮区域和边框的间隔。当值 ≥ margin 的值时，高亮颜色会覆盖整个选中的候选区域，实现「天圆地方」的效果。参考文件末的配置
  "style/layout/hilite_spacing": 5         # 候选和标签的间隔
  "style/layout/hilite_padding_x": 8       # 高亮区域和边框的 X 轴间隔
  "style/layout/hilite_padding_y": 8       # 高亮区域和边框的 Y 轴间隔
  "style/layout/margin_x": 8               # 元素偏离候选边框 X 轴距离；为负值时，不显示候选框
  "style/layout/margin_y": 8               # 元素偏离候选边框 Y 轴距离；为负值时，不显示候选框
  "style/layout/corner_radius": 5         # 候选框圆角
  "style/layout/round_corner": 0          # 高亮区域圆角
  "style/layout/spacing": 10               # 默认间隙
